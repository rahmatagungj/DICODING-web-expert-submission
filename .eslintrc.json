{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 2, { "SwitchCase": 1 }],
        "no-console": 0,
        "linebreak-style": 0,
        "consistent-return": 0,
        "no-prototype-builtins": 0
    }
}
